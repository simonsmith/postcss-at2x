{
  "name": "postcss-at2x",
  "version": "2.0.0",
  "description": "Adds at-2x keyword to background and background-image declarations to add retina support for images.",
  "keywords": [
    "postcss",
    "css",
    "postcssplugin",
    "at2x",
    "retina"
  ],
  "author": "Simon Smith <me@simonsmith.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonsmith/postcss-at2x.git"
  },
  "main": "build/index.js",
  "dependencies": {
    "postcss": "^5.0.4",
    "string.prototype.includes": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "3.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "jshint-stylish": "2.0.1",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "lint": "eslint lib/index.js test/test.js",
    "build": "babel lib/index.js --out-file build/index.js",
    "test": "npm run lint && mocha --compilers js:babel-register",
    "prepublish": "npm run build"
  }
}
